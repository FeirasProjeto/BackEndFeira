{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "index.ts"
        }
    ],
    "crons": [
        {
            "path": "/crons/deletaAntigas",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/crons/atualizaProximaOcorrencia",
            "schedule": "1 * * * *"
        }
    ]
}